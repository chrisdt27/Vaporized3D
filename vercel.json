{
  "buildCommand": "git lfs install && git lfs pull",
  "installCommand": "git lfs install",
  "outputDirectory": ".",
  "functions": {
    "**/*.js": {
      "maxDuration": 30
    }
  },
  "headers": [
    {
      "source": "/(.*)\\.mp4",
      "headers": [
        {
          "key": "Content-Type",
          "value": "video/mp4"
        }
      ]
    },
    {
      "source": "/(.*)\\.glb",
      "headers": [
        {
          "key": "Content-Type", 
          "value": "model/gltf-binary"
        }
      ]
    },
    {
      "source": "/(.*)\\.mp3",
      "headers": [
        {
          "key": "Content-Type",
          "value": "audio/mpeg"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/vaporized3D.html"
    }
  ]
}
